@model ImportTreesModel
@for (int site = 0; site < Model.Sites.Count; site++) {
  for (int tree = 0; tree < Model.Sites[site].Trees.Count; tree++) {
    if (Model.Sites[site].Trees[tree].Id.Equals(ViewBag.treeId)) {
      if (Model.Sites[site].Trees[tree].IsEditing) {
          @Html.EditorFor(m => m.Sites[site].Trees[tree])
      } else {
          @Html.DisplayFor(m => m.Sites[site].Trees[tree])
      }
    }
  }
}
