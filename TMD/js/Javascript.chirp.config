<?xml version="1.0"?>
<root
  xmlns="urn:ChirpyConfig"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:ChirpyConfig http://www.weirdlover.com/chirpy/chirp.xsd">

  <FileGroup Name="Shared.js" Minify="False">
    <File Path="Shared/Extensions.js" />
    <File Path="Shared/Widgets.js" />
  </FileGroup>
  <FileGroup Name="Shared.min.js" Minify="True">
    <File Path="Shared/Extensions.js" />
    <File Path="Shared/Widgets.js" />
  </FileGroup>

  <FileGroup Name="Import.js" Minify="False">
    <File Path="Import/Sites.js" />
