  <FileGroup Name="Import.min.js" Minify="True">
    <File Path="Import/Sites.js" />
    <File Path="Import/Trees.js" />
  </FileGroup>

  <FileGroup Name="Map.js" Minify="False">
    <File Path="Map/CoordinatePicker.js" />
    <File Path="Map/Coordinates.js" />
    <File Path="Map/Extensions.js" />
    <File Path="Map/Widgets.js" />
  </FileGroup>
  <FileGroup Name="Map.min.js" Minify="True">
