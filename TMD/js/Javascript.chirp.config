  <FileGroup Name="Plugins.min.js" Minify="True">
    <File Path="jquery/facebox.js" />
    <File Path="misc/excanvas.js" />
    <File Path="jquery/jquery.dataTables.js" />
    <File Path="jquery/jquery.quicksearch.js" />
    <File Path="jquery/jquery.tablesorter.js" />
    <File Path="jquery/jquery.tipsy.js" />
    <File Path="jquery/jquery.uniform.js" />
    <File Path="jquery/jquery.visualize.js" />
